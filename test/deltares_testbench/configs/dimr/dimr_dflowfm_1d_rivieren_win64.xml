            <maxRunTime>15.0</maxRunTime>
            <checks>
                <file name="dflowfm/output/FlowFM_his.nc" type="netCDF">
                    <parameters>
                        <parameter name="discharge_magnitude" toleranceAbsolute="0.0001" />
                        <parameter name="waterdepth" toleranceAbsolute="0.0001" />
