        <testCase name="e02_f008_c044_bifurcation_equal_length_pure1d" ref="dflowfm_default">
        <path version="2024-11-01T13:00:00">e02_dflowfm/f008_network/c044_bifurcation_equal_length_pure1d</path>
        <maxRunTime>15.0</maxRunTime>
        <checks>
            <file name="dflowfm/output/FlowFM_his.nc" type="netCDF">
