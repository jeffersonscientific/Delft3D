CONSTANTS Nothreads          DATA  1                       ; Nr of cores to be used (1 is recommended)
CONSTANTS Only_Active        DATA  1                       ; Presence of this constant will avoid automatic activation of processes

CONSTANTS Active_EM_GSS      DATA  1                       ; Presence of this constant activates D-Emissions main process
  
                                                           ; grid data from WFLOW (compulsory!), river cells and flow direction
PARAMETERS River      ALL BINARY_FILE ..\staticdata\river.dat
PARAMETERS Downstream ALL BINARY_FILE ..\staticdata\ptiddown.dat

                                                           ; land use (compulsory!)
INCLUDE '..\config\B7_geometry-parameters.inc'
ALL BINARY_FILE '..\config\B7_geometry.bin'

                                                           ; hydrology (compulsory!)
INCLUDE '..\config\B7_hydrology.inc'
ALL BINARY_FILE '..\dynamicdata\hydrology.bin'

                                                           ; erosion (optioal, from wflow sediment model)
INCLUDE '..\config\B7_sediment.inc'
ALL BINARY_FILE '..\dynamicdata\sediment.dat'

                                                           ; meteo (optional, from wflow model)
INCLUDE '..\config\B7_climate.inc'
ALL BINARY_FILE '..\dynamicdata\climate.dat'

;=======================================================================================================
; NOW FOLLOWS THE USER DEFINED INPUT DATA, BELOW THE OBLIGATORY INPUT                        
; NOTE THAT D-EMISSIONS PROVIDES DEFAULTS FOR ALL INPUT ITEMS NOT LISTED BELOW
; THESE DEFAULTS ARE HOWEVER NOT BY DEFINITION MEANINGFUL
; SOME MEANINGLESS SOURCE INFORMATION IS ADDED SO THAT EVEN THIS TEMPLATE WILL PROVIDE SOME RESULTS
;=======================================================================================================

; releases TO BE ADDED

; atmospheric deposition

CONSTANTS EF_DDp   DATA       0.002     ; dry deposition rate                                    (g/m2/d)            
CONSTANTS EF_WDp   DATA       0.300     ; wet deposition rate (concentration in rainfall)        (g/m3)              

; source B01, mimicking population

CONSTANTS EV_B01   DATA       10.       ; emission explaining variable, people per grid cell
CONSTANTS EF_B01   DATA       0.009     ; emission factor in kg/d per EV unit
CONSTANTS B01toWW  DATA       1.000     ; source allocated to wastewater, true destiation dependent on wastweater management

	; management of wastewater and stormwater EXAMPLES

	CONSTANTS FrSewered  DATA       0.80000     ; fraction WW    to sewer systems                        (-)                 
	CONSTANTS FrRainSew  DATA       0.60000     ; fraction    SW to sewer systems                        (-)                 
	CONSTANTS fComSew    DATA      0.700000     ; fraction of combined sewers                            (-)                 

	CONSTANTS SewLeakage DATA       0.02000     ; sewer leakage / CSO definition                         (- or mm)           
	CONSTANTS FrTreat1   DATA       0.30000     ; fraction WW primary treated                            (-)                 
	CONSTANTS FrTreat2   DATA       0.60000     ; fraction WW secondary treated                          (-)                 
	CONSTANTS FrTreat3   DATA       0.00000     ; fraction WW tertiary treated                           (-)                 
	CONSTANTS Eff_Treat1 DATA       0.40000     ; fracion to effluent for primary treatment              (-)                 
	CONSTANTS Eff_Treat2 DATA       0.10000     ; fracion to effluent for secondary treatment            (-)                 
	CONSTANTS Eff_Treat3 DATA       0.10000     ; fracion to effluent for tertiary treatment             (-)                 
	CONSTANTS Sld_Treat1 DATA       0.50000     ; fracion to sludge for primary treatment                (-)                 
	CONSTANTS Sld_Treat2 DATA       0.30000     ; fracion to sludge for secondary treatment              (-)                 
	CONSTANTS Sld_Treat3 DATA       0.30000     ; fracion to sludge for tertiary treatment               (-)                 

	CONSTANTS Eff_RS     DATA       0.90000     ; Fraction to effluent of rain sewers influent           (-)                 
	CONSTANTS Sld_RS     DATA       0.10000     ; Fraction to sludge   of rain sewers influent           (-)                 

	; Pathways in the environment TO BE ADDED

	CONSTANTS kBurial    DATA       0.02000     ; burial rate of unpaved pool                            (-)                 
	CONSTANTS DecPav     DATA       0.01000     ; decay rate paved                                       (/d)                
	CONSTANTS DecUnp     DATA       0.03000     ; decay rate unpaved                                     (/d)                
	CONSTANTS KdUnpa     DATA       1.00000     ; fraction of bound vs unbound                           (-)                 
	CONSTANTS DecSoi     DATA       0.01000     ; decay rate soils                                       (/d)                
	CONSTANTS KdSoi      DATA       1.00000     ; fraction of bound vs unbound                           (-)                 
	CONSTANTS CBack      DATA       0.00200     ; Concentration from pas
                                                           ; definition of soil system (compulsory)
PARAMETERS SoilThick     ALL BINARY_FILE ..\staticdata\soilthickness.dat 
PARAMETERS SoilPoros     ALL BINARY_FILE ..\staticdata\porosity.dat 

