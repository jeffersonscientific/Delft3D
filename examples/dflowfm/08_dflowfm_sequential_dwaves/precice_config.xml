    <write-data name="rtp" mesh="wave_nodes" />
    <write-data name="hrms" mesh="wave_nodes" />
    <write-data name="tp" mesh="wave_nodes" />
    <write-data name="pdir" mesh="wave_nodes" />
    <write-data name="windu" mesh="wave_nodes" />
    <write-data name="windv" mesh="wave_nodes" />
    <write-data name="tps" mesh="wave_nodes" />
    <write-data name="tm02" mesh="wave_nodes" />
    <write-data name="tmm10" mesh="wave_nodes" />
    <write-data name="dhsign" mesh="wave_nodes" />
    <write-data name="drtm01" mesh="wave_nodes" />
    <write-data name="setup" mesh="wave_nodes" />
    <mapping:linear-cell-interpolation direction="read" from="fm_flow_nodes" to="wave_nodes" constraint="consistent" />
    <receive-mesh name="com_mesh" from="fm" />
    <read-data name="com_data_fm_to_wave" mesh="com_mesh" />
    <write-data name="com_data_wave_to_fm" mesh="com_mesh" />
    <!--<export:vtu directory="precice-exports" />-->
  </participant>

  <m2n:sockets acceptor="fm" connector="wave" exchange-directory=".." />

  <coupling-scheme:serial-explicit>
    <time-window-size value="3600" method="fixed" />
    <max-time value="90000" />
    <participants first="wave" second="fm" />
    <exchange data="bed_levels" mesh="fm_flow_nodes" from="fm" to="wave" initialize="true" />
    <exchange data="water_levels" mesh="fm_flow_nodes" from="fm" to="wave" initialize="true" />
    <exchange data="flow_velocity" mesh="fm_flow_nodes" from="fm" to="wave" initialize="true" />
    <exchange data="wind_velocity" mesh="fm_flow_nodes" from="fm" to="wave" initialize="true" />
    <exchange data="hs" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dir" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dirc" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dirs" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="period" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="depth" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="fx" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="fy" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="wsbodyu" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="wsbodyv" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="mx" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="my" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dissip1" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dissip2" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dissip3" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dissip4" mesh="wave_nodes" from="wave" to="fm" />
