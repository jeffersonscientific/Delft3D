    <use-data name="tm02" />
    <use-data name="tmm10" />
    <use-data name="dhsign" />
    <use-data name="drtm01" />
    <use-data name="setup" />
  </mesh>

  <mesh name="com_mesh" dimensions="2">
    <use-data name="com_data_fm_to_wave" />
    <use-data name="com_data_wave_to_fm" />
  </mesh>

  <mesh name="wave_nodes" dimensions="2">
    <use-data name="bed_levels" />
    <use-data name="water_levels" />
    <use-data name="flow_velocity" />
    <use-data name="wind_velocity" />
    <use-data name="hs" />
    <use-data name="dir" />
    <use-data name="dirc" />
    <use-data name="dirs" />
    <use-data name="period" />
    <use-data name="depth" />
    <use-data name="fx" />
    <use-data name="fy" />
    <use-data name="wsbodyu" />
    <use-data name="wsbodyv" />
    <use-data name="mx" />
    <use-data name="my" />
    <use-data name="dissip1" />
    <use-data name="dissip2" />
    <use-data name="dissip3" />
    <use-data name="dissip4" />
    <use-data name="ubot" />
    <use-data name="steep" />
    <use-data name="wlen" />
    <use-data name="u" />
    <use-data name="v" />
    <use-data name="dspr" />
    <use-data name="rleak" />
    <use-data name="qb" />
    <use-data name="x" />
    <use-data name="y" />
    <use-data name="rtp" />
    <use-data name="hrms" />
    <use-data name="tp" />
    <use-data name="pdir" />
    <use-data name="windu" />
    <use-data name="windv" />
    <use-data name="tps" />
    <use-data name="tm02" />
    <use-data name="tmm10" />
    <use-data name="dhsign" />
    <use-data name="drtm01" />
    <use-data name="setup" />
  </mesh>

  <participant name="fm">
    <provide-mesh name="fm_flow_nodes" />
    <receive-mesh name="wave_nodes" from="wave" />
    <write-data name="bed_levels" mesh="fm_flow_nodes" />
    <write-data name="water_levels" mesh="fm_flow_nodes" />
    <write-data name="flow_velocity" mesh="fm_flow_nodes" />
    <write-data name="wind_velocity" mesh="fm_flow_nodes" />
    <read-data name="hs" mesh="fm_flow_nodes" />
    <read-data name="dir" mesh="fm_flow_nodes" />
    <read-data name="dirc" mesh="fm_flow_nodes" />
    <read-data name="dirs" mesh="fm_flow_nodes" />
    <read-data name="period" mesh="fm_flow_nodes" />
    <read-data name="depth" mesh="fm_flow_nodes" />
    <read-data name="fx" mesh="fm_flow_nodes" />
    <read-data name="fy" mesh="fm_flow_nodes" />
    <read-data name="wsbodyu" mesh="fm_flow_nodes" />
    <read-data name="wsbodyv" mesh="fm_flow_nodes" />
    <read-data name="mx" mesh="fm_flow_nodes" />
    <read-data name="my" mesh="fm_flow_nodes" />
    <read-data name="dissip1" mesh="fm_flow_nodes" />
    <read-data name="dissip2" mesh="fm_flow_nodes" />
    <read-data name="dissip3" mesh="fm_flow_nodes" />
    <read-data name="dissip4" mesh="fm_flow_nodes" />
    <read-data name="ubot" mesh="fm_flow_nodes" />
    <read-data name="steep" mesh="fm_flow_nodes" />
    <read-data name="wlen" mesh="fm_flow_nodes" />
    <read-data name="u" mesh="fm_flow_nodes" />
    <read-data name="v" mesh="fm_flow_nodes" />
    <read-data name="dspr" mesh="fm_flow_nodes" />
    <read-data name="rleak" mesh="fm_flow_nodes" />
    <read-data name="qb" mesh="fm_flow_nodes" />
    <read-data name="x" mesh="fm_flow_nodes" />
    <read-data name="y" mesh="fm_flow_nodes" />
    <read-data name="rtp" mesh="fm_flow_nodes" />
    <read-data name="hrms" mesh="fm_flow_nodes" />
    <read-data name="tp" mesh="fm_flow_nodes" />
    <read-data name="pdir" mesh="fm_flow_nodes" />
    <read-data name="windu" mesh="fm_flow_nodes" />
    <read-data name="windv" mesh="fm_flow_nodes" />
    <read-data name="tps" mesh="fm_flow_nodes" />
    <read-data name="tm02" mesh="fm_flow_nodes" />
    <read-data name="tmm10" mesh="fm_flow_nodes" />
    <read-data name="dhsign" mesh="fm_flow_nodes" />
    <read-data name="drtm01" mesh="fm_flow_nodes" />
    <read-data name="setup" mesh="fm_flow_nodes" />
    <mapping:linear-cell-interpolation direction="read" from="wave_nodes" to="fm_flow_nodes" constraint="consistent" />
    <provide-mesh name="com_mesh" />
    <write-data name="com_data_fm_to_wave" mesh="com_mesh" />
    <read-data name="com_data_wave_to_fm" mesh="com_mesh" />
  </participant>

  <participant name="wave">
    <provide-mesh name="wave_nodes" />
    <receive-mesh name="fm_flow_nodes" from="fm" />
    <read-data name="bed_levels" mesh="wave_nodes" />
    <read-data name="water_levels" mesh="wave_nodes" />
    <read-data name="flow_velocity" mesh="wave_nodes" />
    <read-data name="wind_velocity" mesh="wave_nodes" />
    <write-data name="hs" mesh="wave_nodes" />
    <write-data name="dir" mesh="wave_nodes" />
    <write-data name="dirc" mesh="wave_nodes" />
    <write-data name="dirs" mesh="wave_nodes" />
    <write-data name="period" mesh="wave_nodes" />
    <write-data name="depth" mesh="wave_nodes" />
    <write-data name="fx" mesh="wave_nodes" />
    <write-data name="fy" mesh="wave_nodes" />
    <write-data name="wsbodyu" mesh="wave_nodes" />
    <write-data name="wsbodyv" mesh="wave_nodes" />
    <write-data name="mx" mesh="wave_nodes" />
    <write-data name="my" mesh="wave_nodes" />
    <write-data name="dissip1" mesh="wave_nodes" />
    <write-data name="dissip2" mesh="wave_nodes" />
    <write-data name="dissip3" mesh="wave_nodes" />
    <write-data name="dissip4" mesh="wave_nodes" />
    <write-data name="ubot" mesh="wave_nodes" />
    <write-data name="steep" mesh="wave_nodes" />
    <write-data name="wlen" mesh="wave_nodes" />
