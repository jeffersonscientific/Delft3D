    <exchange data="steep" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="wlen" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="u" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="v" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dspr" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="rleak" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="qb" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="x" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="y" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="rtp" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="hrms" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="tp" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="pdir" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="windu" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="windv" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="tps" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="tm02" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="tmm10" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="dhsign" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="drtm01" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="setup" mesh="wave_nodes" from="wave" to="fm" />
    <exchange data="com_data_fm_to_wave" mesh="com_mesh" from="fm" to="wave" />
    <exchange data="com_data_wave_to_fm" mesh="com_mesh" from="wave" to="fm" />
  </coupling-scheme:serial-explicit>
</precice-configuration>
